
config interface   'loopback'
	option ifname  'lo'
	option proto   'static'
	option ipaddr  '127.0.0.1'
	option netmask '255.0.0.0'

config interface   'public'
    option type    'bridge'
    option proto   'dhcp'

config interface   'wan'
    option type    'bridge'
    option ifname  'eth0 eth1'
    option proto   'dhcp'

